config_version=5

[application]

config/name="OverJuiced Infinity Tic-Tac-Toe"
config/version="1.0"
run/main_scene="res://Menus/Menu.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://AppIcon.png"

[autoload]

Transition="*res://Transitions/Transition.tscn"

[display]

window/size/viewport_width=600
window/size/viewport_height=1066
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/handheld/orientation=1

[dotnet]

project/assembly_name="Tic-Time-Toe"

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
